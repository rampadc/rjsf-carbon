{
  "name": "rjsf-carbon",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "type": "module",
  "description": "Carbon Design System theme for react-jsonschema-form",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./dist/index.js",
      "import": "./lib/index.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./dist/index.js",
      "import": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./dist/*.js",
      "import": "./lib/*.js"
    },
    "./dist": {
      "types": "./lib/index.d.ts",
      "require": "./dist/index.js",
      "import": "./lib/index.js"
    },
    "./dist/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./dist/*.js",
      "import": "./lib/*.js"
    }
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjsf-team/react-jsonschema-form.git"
  },
  "scripts": {
    "build:ts": "tsc -b tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:cjs": "esbuild ./src/index.ts --bundle --outfile=dist/index.js --sourcemap --packages=external --format=cjs",
    "build:esm": "esbuild ./src/index.ts --bundle --outfile=dist/rjsf-carbon.esm.js --sourcemap --packages=external --format=esm",
    "build:umd": "rollup dist/rjsf-carbon.esm.js --format=umd --file=dist/rjsf-carbon.umd.js --name=@rjsf/carbon --globals react:React,@carbon/react:CarbonReact",
    "build:scss": "sass src/style.scss:dist/style.css --style=compressed --load-path=node_modules --quiet",
    "postbuild:scss": "cp dist/style.css lib/style.css",
    "build": "npm run build:ts && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:scss"
  },
  "lint-staged": {
    "{src,test}/**/*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@carbon/react": "^1.82.1",
    "@carbon/styles": "^1.81.0",
    "@rjsf/core": "^6.x",
    "@rjsf/utils": "^6.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.72.0"
  },
  "engineStrict": false,
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@carbon/react": "^1.82.1",
    "@carbon/styles": "^1.81.0",
    "@rjsf/core": "^6.0.0-beta.1",
    "@rjsf/snapshot-tests": "^6.0.0-beta.1",
    "@rjsf/utils": "^6.0.0-beta.1",
    "@rjsf/validator-ajv8": "^6.0.0-beta.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.40.2",
    "sass": "^1.72.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-jsonschema-form",
    "jsonschema",
    "json-schema",
    "json",
    "schema",
    "form",
    "react",
    "carbon",
    "carbon-design-system"
  ],
  "contributors": [],
  "license": "Apache-2.0"
}
